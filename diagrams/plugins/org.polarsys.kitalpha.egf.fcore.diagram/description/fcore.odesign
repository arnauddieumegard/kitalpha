      <ownedLineMappings name="PatternLine" domainClass="pattern::Pattern" semanticCandidatesExpression="aql:self.libraries->closure(e | e.elements.superPattern.container.eContainer().libraries.elements)" headerLabelExpression="aql:self.container.name + '.' + self.name">
