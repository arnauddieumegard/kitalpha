      <filters xsi:type="filter:CompositeFilterDescription" name="CustomMethodsFilter">
        <filters xsi:type="filter:MappingFilter" mappings="//@ownedViewpoints[name='fcore%20analysis']/@ownedRepresentations[name='DiagramPatternHierarchy']/@defaultLayer/@containerMappings[name='ContainerPatternLibrary']/@subContainerMappings[name='ContainerListPattern']/@subNodeMappings[name='CustomNodeMethods']"/>
      </filters>
      <filters xsi:type="filter:CompositeFilterDescription" name="DefaultMethodsFilter">
        <filters xsi:type="filter:MappingFilter" mappings="//@ownedViewpoints[name='fcore%20analysis']/@ownedRepresentations[name='DiagramPatternHierarchy']/@defaultLayer/@containerMappings[name='ContainerPatternLibrary']/@subContainerMappings[name='ContainerListPattern']/@subNodeMappings[name='DefaultNodeMethods']"/>
      </filters>
      <filters xsi:type="filter:CompositeFilterDescription" name="VariablesFilter">
